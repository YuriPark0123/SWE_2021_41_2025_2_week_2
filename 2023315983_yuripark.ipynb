        "def isHappy(n):\n",
        "\n",
        "  while n >= 10:\n",
        "    str_n = str(n)\n",
        "    n = sum(int(digit)**2 for digit in str_n)\n",
        "\n",
        "  return n ==1\n",
        "\n",
        "print(isHappy(19))\n",
        "print(isHappy(1))\n",
        "print(isHappy(2))\n"
      ]
    }
